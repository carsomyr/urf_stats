<div class="container-fluid">
    <div class="row">
        <h2 class="page-item">Champion Duos</h2>

        <div class="data-table page-item">
            <div class="data-header">
                <div class="data-cell" style="width: 25%;">Champion Duo</div>
                <div class="data-cell" style="width: 25%;"># Victims</div>
                <div class="data-cell" style="width: 25%;"># Matches</div>
                <div class="data-cell" style="width: 25%;">Avg Victims/Match</div>
            </div>
            <div class="data-body">
                {{#each championDuoStat in controller.model}}
                    <div class="data-row">
                        <div class="data-cell" data-label="Champion Duo">
                            <div class="image-holder">
                                {{#each champion in championDuoStat.champions}}
                                    {{static-entity-image model=champion}}
                                {{/each}}
                            </div>
                        </div>
                        <div class="data-cell" data-label="# Victims">
                            {{championDuoStat.nKillAssists}}
                        </div>
                        <div class="data-cell" data-label="# Matches">
                            {{championDuoStat.nMatches}}
                        </div>
                        <div class="data-cell" data-label="Avg Victims/Match">
                            {{championDuoStat.averageKillAssistsPerMatch}}
                        </div>
                    </div>
                {{/each}}
            </div>
        </div>
    </div>
</div>
